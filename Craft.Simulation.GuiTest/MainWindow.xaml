<Window x:Class="Craft.Simulation.GuiTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Craft.Simulation.GuiTest"
        xmlns:tab1="clr-namespace:Craft.Simulation.GuiTest.Tab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Closing="MainWindow_OnClosing">
    <TabControl>
        <TabItem Header="Bouncing Ball">
            <tab1:Tab1View DataContext="{Binding Tab1ViewModel}"/>
        </TabItem>
    </TabControl>
</Window>
